/* 顶部要固定定位   标题要居中   自定义按钮和标题要和右边微信原生的胶囊上下对齐 */

.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  color: #000;
  z-index: 9999999;
}
/* 标题要居中 */

.nav-title {
  position: absolute;
  text-align: center;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 18px;
  color: #2c2b2b;
  font-weight: bold;
}
.nav{
  position: absolute;
  top: 28px;
  left: 30rpx;
  height: 60rpx;
  overflow: hidden;
  border: 1rpx solid #eeeeee;
  border-radius: 30rpx;
}
.line{
  float: left;
  height: 30rpx;
  margin-top:12rpx; 
  border: 1rpx solid #eeeeee;
}
.nav_back,.nav_home{
  margin-top: 10rpx;
  margin-left: 20rpx;
  margin-right: 20rpx; 
  width: 40rpx;
  height:40rpx;
  float: left;
}
image{
  width: 100%;
  height:100%;
}
.downtips {
  height: 0px;
  float: left;
  border-left: 5px solid transparent;
  border-right: 5px solid transparent;
  border-top: 5px solid #000;
  margin-left: 2rpx;
}
/* 
view {
  border: 1px solid #000;
} */

.nav-city {
  width: 150rpx;
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  font-weight: bold;
  font-size: 22px;
}

.section {
  font-size: 12px;
  background-color: #f5f8f8;
  position: absolute;
  height: 30px;
  overflow: hidden;
  border-radius: 15px;
  text-align: center;
  width: 360rpx;
  white-space: nowrap;
  right: 20px;
  left: 0;
  margin: auto;
}
.search_icon{
  margin-left: 12px;
  margin-top: 9px;
  float: left;
  width: 14px;
  height: 14px;
}
.placehold{
  float: left;
  margin-top: 7px;
  margin-left: 5px;
  color: #717476;
}
