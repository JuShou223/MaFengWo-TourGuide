/**index.wxss**/
page,.main,.header_bdr,.main_scro,.list,.must_tiyan,image{
  height: 100%;
}
.header,.menu_list,image,.views,.img,.travel_guide,.must_tiyan,.tg_data{
  width: 100%;
}
.downtips,.highlight,.view_item,.label,.tg_img,.tg_info_title,
.tg_info,.tg_info_read,.tg_info_collects,.tg_data{
  float: left;
}
.header_show,.header_show,.header_time,.header_showpeople,.header_avatars,.header_avatar{
  float: right;
}
.header,.header_bdr,.header_title,.menu_list,.tg_data{
  display: flex;
}
.header,.highlight_list,.views,.view,.img,.main_scro,.tg_img,.travel_guide,.travel_guide_content{
  overflow: hidden;
}
.header {
  height: 160rpx;
  background-color: #354a98;
}
