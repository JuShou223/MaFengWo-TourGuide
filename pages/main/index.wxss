.header_bdr {
  justify-content: left;
  align-items: center;
}

.header_bdra {
  height: 90rpx;
  border-left: 6rpx solid #ffdc1e;
  border-right: 6rpx solid #ffdc1e;
  border-radius: 0 9rpx 9rpx 0;
}

.header .header_hd {
  width: 45%;
}

.header .header_title {
  margin-top: 20rpx;
  padding-left: 24rpx;
  font-size: 28rpx;
  color: #fff;
  opacity: 0.6;
  align-items: center;
}

.dot {
  width: 2rpx;
  height: 2rpx;
  border: 2rpx solid #eaeaea;
  border-radius: 50%;
  margin-left: 2rpx;
  background-color: #eaeaea;
}

.header .header_city {
  color: #fff;
  font-size: 64rpx;
  padding-left: 24rpx;
  align-items: center;
  white-space: nowrap;
  overflow: hidden;
  max-width: 246rpx;
  text-overflow: ellipsis;
  float: left;
}

.downtips {
  height: 0px;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 10rpx solid #eaeaea;
  margin-top: 40rpx;
  float: left;
  margin-left: 8rpx;
}

.header_show {
  width: 59%;
}

.header_time {
  margin-right: 15px;
  background: #2a3c77;
  padding: 4rpx 10rpx;
  color: #fff;
  font-size: 20rpx;
  border-radius: 10rpx;
}

.header_showpeople {
  margin: 8rpx 0 12rpx;
  margin-right: 15px;
  width: 80%;
  text-align: right;
  color: #fff;
  font-size: 28rpx;
}

.header_avatars {
  position: relative;
  width: 50%;
  height: 50rpx;
  margin-right: 15px;
}

.header_avatar {
  border: 2rpx solid #fff;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  position: absolute;
  overflow: hidden;
}

.a1 {
  right: 0;
}

.a2 {
  right: 40rpx;
}

.a3 {
  right: 80rpx;
}

.a4 {
  right: 120rpx;
}
.a5 {
  right: 160rpx;
}
.menu_list {
  border-bottom: 2rpx solid #eaeaea;
  margin-bottom: 20rpx;
}
.list {
  padding:20rpx 0; 
  text-align: center;
  flex: 1;
  font-size: 36rpx;
  color: #6e757b;
}
.on {
  border-bottom: 2rpx solid #ffdc1e;
  color: black;
}
.highlight_list{
  width: 80%;
  margin-left: 20rpx;
}
.highlight{
